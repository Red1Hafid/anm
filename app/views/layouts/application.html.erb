<!DOCTYPE html>
<html>
  <head>
    <title>AnmFurloughs</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <!-- App favicon -->
    <link rel="shortcut icon" href=<%= "#{domainename}assets/images/favicon.ico" %>>

    <!-- App css -->
    <link href=<%= "#{domainename}assets/libs/dropzone/min/dropzone.min.css" %> rel="stylesheet" type="text/css" />
    <link href=<%= "#{domainename}assets/libs/dropify/css/dropify.min.css" %> rel="stylesheet" type="text/css" />

    <link href=<%= "#{domainename}assets/libs/bootstrap-table/bootstrap-table.min.css" %> rel="stylesheet" type="text/css" />
    <link href=<%= "#{domainename}assets/libs/flatpickr/flatpickr.min.css" %> rel="stylesheet" type="text/css" />

    <link href=<%= "#{domainename}assets/libs/datatables.net-bs4/css/dataTables.bootstrap4.min.css" %> rel="stylesheet" type="text/css" />
    <link href=<%= "#{domainename}assets/libs/datatables.net-responsive-bs4/css/responsive.bootstrap4.min.css" %> rel="stylesheet" type="text/css" />

    <!-- App css -->
    <link href=<%= "#{domainename}assets/css/bootstrap-material.min.css" %> rel="stylesheet" type="text/css" id="bs-default-stylesheet" />
    <link href=<%= "#{domainename}assets/css/app-material.min.css" %> rel="stylesheet" type="text/css" id="app-default-stylesheet" />

    <link href=<%= "#{domainename}assets/css/bootstrap-material-dark.min.css" %> rel="stylesheet" type="text/css" id="bs-dark-stylesheet" disabled />
    <link href=<%= "#{domainename}assets/css/app-material-dark.min.css" %> rel="stylesheet" type="text/css" id="app-dark-stylesheet"  disabled />
    
    <!-- Jquery Toast css -->
    <link href=<%= "#{domainename}assets/libs/jquery-toast-plugin/jquery.toast.min.css" %> rel="stylesheet" type="text/css" />
    <link href=<%= "#{domainename}assets/css/bootstrap.min.css" %> rel="stylesheet" type="text/css" id="bs-default-stylesheet" />
    <link href=<%= "#{domainename}assets/css/app.css" %> rel="stylesheet" type="text/css" id="app-default-stylesheet" />

    <link href=<%= "#{domainename}assets/css/bootstrap-dark.min.css" %> rel="stylesheet" type="text/css" id="bs-dark-stylesheet" disabled />
    <link href=<%= "#{domainename}assets/css/app-dark.min.css" %> rel="stylesheet" type="text/css" id="app-dark-stylesheet"  disabled />
    <!-- icons -->
    <link href=<%= "#{domainename}assets/css/icons.min.css" %> rel="stylesheet" type="text/css" />

   <link href=<%= "#{domainename}assets/libs/mohithg-switchery/switchery.min.css" %> rel="stylesheet" type="text/css" />
   <link href=<%= "#{domainename}assets/libs/multiselect/css/multi-select.css" %> rel="stylesheet" type="text/css" />
   <link href=<%= "#{domainename}assets/libs/select2/css/select2.min.css" %> rel="stylesheet" type="text/css" />
   <link href=<%= "#{domainename}assets/libs/selectize/css/selectize.bootstrap3.css" %> rel="stylesheet" type="text/css" />
   <link href=<%= "#{domainename}assets/libs/bootstrap-select/css/bootstrap-select.min.css" %> rel="stylesheet" type="text/css" />
   <link href=<%= "#{domainename}assets/libs/bootstrap-touchspin/jquery.bootstrap-touchspin.min.css" %> rel="stylesheet" type="text/css" />

   <!-- Plugins css -->
   <link href=<%= "#{domainename}assets/libs/quill/quill.core.css"%> rel="stylesheet" type="text/css" />
   <link href=<%= "#{domainename}assets/libs/quill/quill.bubble.css"%> rel="stylesheet" type="text/css" />

    <link href=<%= "#{domainename}assets/libs/@fullcalendar/core/main.min.css" %> rel="stylesheet" type="text/css" />
    <link href=<%= "#{domainename}assets/libs/@fullcalendar/daygrid/main.min.css" %> rel="stylesheet" type="text/css" />
    <link href=<%= "#{domainename}assets/libs/@fullcalendar/bootstrap/main.min.css" %> rel="stylesheet" type="text/css" />
    <link href=<%= "#{domainename}assets/libs/@fullcalendar/timegrid/main.min.css" %> rel="stylesheet" type="text/css" />
    <link href=<%= "#{domainename}assets/libs/@fullcalendar/list/main.min.css" %> rel="stylesheet" type="text/css" />

    <!-- Calendar init -->
    <script src="../assets/js/pages/calendar.init.js"></script>

    <!-- App css -->
    <link href=<%= "#{domainename}assets/css/app.min.css" %> rel="stylesheet" type="text/css" id="app-default-stylesheet" />
    
    <%= stylesheet_link_tag 'application', media: 'all' %>
    
  </head>

  <body class="<%= 'authentication-bg authentication-bg-pattern' if yield(:title) == 'login' %>" onload= 'get_date_from_and_to();' >
  

    <% if yield(:title) == "login" %>
        <%= yield %>
    <% else %>

      <div id="wrapper">
        <% if user_signed_in? %>
          <%= render 'layouts/navmain' %>
          <%= render 'layouts/sidebar' %>
        <% end %>
        
        <div class="content-page">
            <%= yield %>
            <% if !["furloughs", "additional_hours"].include?(yield(:title)) %>
              <%= render 'layouts/footer' %>  
            <% end %>
        </div>
      </div> 

    <% end %>

     <%= javascript_include_tag 'application' %>

    <!-- Vendor JavaScripts -->

    <!-- Vendor js -->
    <script src=<%= "#{domainename}assets/js/vendor.min.js" %> ></script>
    <script src=<%= "#{domainename}assets/libs/dropzone/min/dropzone.min.js" %> ></script>
    <script src=<%= "#{domainename}assets/libs/dropify/js/dropify.min.js" %> ></script>
    <script src=<%= "#{domainename}assets/js/pages/form-fileuploads.init.js" %> ></script>
     <!-- Plugins js-->

    <!-- Tost-->
    <script src=<%= "#{domainename}assets/libs/jquery-toast-plugin/jquery.toast.min.js" %> ></script>
    <script src=<%= "#{domainename}assets/js/pages/toastr.init.js" %> ></script>
    <!-- App js -->
    <script src=<%= "#{domainename}assets/js/app.min.js" %> ></script>
    <script src=<%= "#{domainename}assets/libs/selectize/js/standalone/selectize.min.js" %>></script>
    <script src=<%= "#{domainename}assets/libs/multiselect/js/jquery.multi-select.js" %>></script>
    <script src=<%= "#{domainename}assets/libs/select2/js/select2.min.js" %>></script>
    <script src=<%= "#{domainename}assets/libs/devbridge-autocomplete/jquery.autocomplete.min.js" %>></script>
    <script src=<%= "#{domainename}assets/libs/bootstrap-select/js/bootstrap-select.min.js" %>></script>
    <script src=<%= "#{domainename}assets/libs/bootstrap-touchspin/jquery.bootstrap-touchspin.min.js" %>></script>
    <script src=<%= "#{domainename}assets/libs/bootstrap-maxlength/bootstrap-maxlength.min.js" %>></script>

    <!-- Init js-->
    <script src=<%= "#{domainename}assets/js/pages/form-advanced.init.js" %>></script>

    <!-- Bootstrap Tables js -->
    <script src=<%= "#{domainename}assets/libs/bootstrap-table/bootstrap-table.min.js" %>></script>

    <!-- Init js -->
    <script src=<%= "#{domainename}assets/js/pages/bootstrap-tables.init.js" %>></script>
    <script src=<%= "#{domainename}assets/libs/dragula/dragula.min.js" %>></script>
    <!-- Dragula init js-->
    <script src=<%= "#{domainename}assets/js/pages/dragula.init.js" %>></script>

    <!-- Plugins js -->
    <script src=<%= "#{domainename}assets/libs/quill/quill.min.js" %>></script>

    <!-- Init js-->
    <script src=<%= "#{domainename}assets/js/pages/task.init.js" %>></script>

    <!-- CRM Dashboard init js-->
    <script src=<%= "#{domainename}assets/js/pages/crm-dashboard.init.js" %>></script>

    <!-- third party js -->
    <script src=<%= "#{domainename}assets/libs/datatables.net/js/jquery.dataTables.min.js" %>></script>
    <script src=<%= "#{domainename}assets/libs/datatables.net-bs4/js/dataTables.bootstrap4.min.js" %>></script>
    <script src=<%= "#{domainename}assets/libs/datatables.net-responsive/js/dataTables.responsive.min.js" %>></script>
    <script src=<%= "#{domainename}assets/libs/datatables.net-responsive-bs4/js/responsive.bootstrap4.min.js" %>></script>
    <!-- third party js ends -->

    <!-- Footable js -->
    <script src=<%= "#{domainename}assets/libs/footable/footable.all.min.js" %>></script>

    <!-- Init js -->
    <script src=<%= "#{domainename}assets/js/pages/foo-tables.init.js" %>></script>

    <!-- Tickets js -->
    <script src=<%= "#{domainename}assets/js/pages/tickets.js" %>></script>

    <%= render 'layouts/flash_furlough' %>
  </body>
</html>